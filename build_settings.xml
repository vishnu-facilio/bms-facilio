<?xml version="1.0" encoding="UTF-8"?>
<Settings>
    <servers>
        <server>
            <id>maven-group</id>
            <username>${repo.name}</username>
            <password>${repo.pwd}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>maven-group</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>maven-group</id>
                    <url>${repo.domain}/repository/maven-group/</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <mirrors>
        <mirror>
            <id>maven-group</id>
            <name>maven-central</name>
            <url>${repo.domain}/repository/maven-group/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
</Settings>