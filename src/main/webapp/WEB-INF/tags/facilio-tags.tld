<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        version="2.0">
	
	<tlibversion>1.2</tlibversion>
	<shortname>Facilio Tags</shortname>
	<info>Custom tags for Facilio (ex: chart)</info>
	<uri>facilio-tags</uri>
	<tag>
		<name>chart</name>
		<tag-class>com.facilio.taglib.ChartTag</tag-class>
		<body-content>empty</body-content>
		<info>chart widget</info>
		<attribute>
			<name>id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>type</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>width</name>
			<required>false</required>
			<type>Integer</type>
		</attribute>
		<attribute>
			<name>height</name>
			<required>false</required>
			<type>Integer</type>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>onmove</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>hasPermission</name>
		<tag-class>com.facilio.taglib.HasPermission</tag-class>
		<info>tag for checking permission</info>
		<attribute>
			<name>permission</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
