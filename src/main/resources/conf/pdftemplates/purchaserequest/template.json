    "type": "purchase-request-line-items",
    "badgable": false,
    "content": "\n<div class=\"line-item-dummy-data dummy-data-wrapper purchase-request\">\n<table class=\"line-item-table line-item-table-overide\">\n  <thead>\n    <tr><th data-key=\"s_no\" class=\"table-field\" style=\"text-align: left !important;\">S.No</th>\n<th data-key=\"item_desc\" class=\"table-field\" style=\"text-align: left !important;\">Item and Description</th>\n<th data-key=\"unit_price\" class=\"table-field\" style=\"text-align: right !important;\">Unit price ({{ org.currency }})</th>\n<th data-key=\"qty\" class=\"table-field\" style=\"text-align: right !important;\">Qty</th>\n<th data-key=\"tax\" class=\"table-field\" style=\"text-align: right !important;\">Tax (%)</th>\n<th data-key=\"uom\" class=\"table-field\" style=\"text-align: right !important;\">UOM</th>\n<th data-key=\"amount\" class=\"table-field\" style=\"text-align: right !important;\">Amount ({{ org.currency }})</th>\n</tr>\n  </thead>\n  <tbody>\n    <tr><td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n<div class=\"table-field-data-shimmer-2\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td></tr>\n<tr><td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n<div class=\"table-field-data-shimmer-2\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td></tr>\n<tr><td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n<div class=\"table-field-data-shimmer-2\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td></tr>\n    \n<tr id=\"controlledRow1\" data-key=\"sub_total\">\n  <td colspan=\"5\" class=\"controlled-row-data\">\n    <div class=\"text-right fc-violet fwBold controlled-row-data-wrapper-2 wt-600\">\n        <div>Sub Total ({{ org.currency }})</div>\n    </div>\n  </td>\n  \n  <td colspan=\"3\" class=\"table-data-field\">\n    <div class=\"table-field-data-shimmer-1\"></div>\n  </td>\n\n</tr>\n<tr id=\"controlledRow2\" data-key=\"discount\">\n  <td colspan=\"5\" class=\"controlled-row-data\">\n    <div class=\"text-right fc-violet fwBold controlled-row-data-wrapper-2 wt-600\">\n        <div>Discount</div>\n    </div>\n  </td>\n  \n  <td colspan=\"3\" class=\"table-data-field\">\n    <div class=\"table-field-data-shimmer-1\"></div>\n  </td>\n\n</tr>\n<tr id=\"controlledRow9\" data-key=\"grand_total\">\n  <td colspan=\"5\" class=\"controlled-row-data-2\">\n    <div class=\"text-right fc-violet text-uppercase fwBold controlled-row-data-wrapper-2 wt-600\">\n        <div>Grand total (