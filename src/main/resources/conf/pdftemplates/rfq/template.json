    "content": "\n  <div class=\"line-item-dummy-data dummy-data-wrapper requestForQuotation\">\n    <table class=\"line-item-table line-item-table-overide\">\n      <thead>\n        <tr><th data-key=\"s_no\" class=\"table-field\" style=\"text-align: left !important;\">S.No</th>\n<th data-key=\"item_desc\" class=\"table-field\" style=\"text-align: left !important;\">Item and Description</th>\n<th data-key=\"qty\" class=\"table-field\" style=\"text-align: right !important;\">Qty</th>\n<th data-key=\"uom\" class=\"table-field\" style=\"text-align: left !important;\">UOM</th>\n<th data-key=\"awarded_to\" class=\"table-field\" style=\"text-align: left !important;\">Awarded To</th>\n<th data-key=\"awarded_price\" class=\"table-field\" style=\"text-align: right !important;\">Awarded price</th>\n<th data-key=\"tax_amount\" class=\"table-field\" style=\"text-align: right !important;\">Tax Amount</th>\n<th data-key=\"amount\" class=\"table-field\" style=\"text-align: right !important;\">Amount</th>\n</tr>\n      </thead>\n      <tbody>\n        <tr><td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n<div class=\"table-field-data-shimmer-2\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td></tr>\n<tr><td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\"></div>\n</td>\n<td class=\"table-field-data\">\n<div class=\"table-field-data-shimmer-1\">