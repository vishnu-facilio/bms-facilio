<?xml version="1.0" encoding="UTF-8"?>
<config>
    <request path="/api/setup/adduser">
        <requestbody type="object">
            <property name="appId" type="number"/>
            <property name="emailVerificationNeeded" type="boolean" />
            <property name="user" type="object">
                <property name="email" type="string"/>
                <property name="language" type="string"/>
                <property name="name" type="string" />
                <property name="roleId" type="number" />
            </property>
        </requestbody>
    </request>

    <request path="/api/v3/modules/data/create">
        <requestbody type="v3fields" />
    </request>

    <request path="/api/v2/statetransition/timelog">
        <requestbody type="object">
            <property name="moduleName" type="string"/>
            <property name="id" type="number"/>
        </requestbody>
    </request>

    <request path="/api/v2/feedbackType/add">
        <requestbody type="object">
            <property name="feedbackType" type="object">
                <property name="name" type="string" maxLength="40" mandatory="true"/>
            </property>
        </requestbody>
    </request>
    <request path="/api/v2/feedbackKiosk/add">
        <requestbody type="object">
            <property name="feedbackKiosk" type="object">
                <property name="name" type="string" mandatory="true" maxLength="255"/>
                <property name="description" type="string" maxLength="2040"/>
                <property name="feedbackTypeId" type="number"/>
                <property name="siteId" type="number"/>
                <property name="associatedResource" type="object">
                    <property name="id" type="number"/>
                </property>
            </property>
        </requestbody>
    </request>
    <request path="/api/setup/addgroup">
        <requestbody type="object">
            <property name="group" type="object">
                <property name="name" type="string" mandatory="true" maxLength="255"/>
                <property name="description" type="string" maxLength="1000"/>
                <property name="siteId" type="number"/>
                <property name="email" type="string" maxLength="255"/>
                <property name="members" type="array">
                    <items type="number"/>
                </property>
            </property>
        </requestbody>
    </request>
    <request path="/api/setup/addRole">
        <requestbody type="object">
            <property name="role" type="object">
                <property name="name" type="string" mandatory="true" maxLength="50"/>
                <property name="description" type="string" maxLength="500"/>
            </property>
            <property name="roleApp" type="array">
                <property name="applicationId" type="number"/>
            </property>
        </requestbody>
    </request>
    <request path="/api/setup/adduser">
        <requestbody type="object">
            <property name="user" type="object">
                <property name="accessibleSpace" type="array">
                    <items type="number"/>
                </property>
                <property name="language" type="string"/>
                <property name="name" type="string" mandatory="true" maxLength="255"/>
                <property name="email" type="string" mandatory="true" maxLength="150" />
                <property name="roleId" type="number" mandatory="true"/>
                <property name="timezone" type="string" maxLength="100"/>
            </property>
        </requestbody>
    </request>
    <request path="/api/v2/updateSecurityPolicy">
        <requestbody type="object">
            <property name="securityPolicy" type="object">
                <property name="asMap" type="object">
                    <property name="name" type="string"/>
                    <property name="description" type="string"/>
                    <property name="id" type="number"/>
                    <property name="isDefault" type="boolean"/>
                    <property name="isTOTPEnabled" type="boolean"/>
                    <property name="isMOTPEnabled" type="boolean"/>
                    <property name="isPwdPolicyEnabled" type="boolean"/>
                    <property name="isWebSessManagementEnabled" type="boolean"/>
                    <property name="orgId" type="number"/>
                    <property name="pwdIsMixed" type="boolean"/>
                    <property name="pwdMinAge" type="number"/>
                    <property name="pwdMinNumDigits" type="number"/>
                    <property name="pwdMinSplChars" type="number"/>
                    <property name="pwdPrevPassRefusal" type="number"/>
                    <property name="pwdMinLength" type="number"/>
                    <property name="webSessLifeTime" type="number"/>
                    <property name="idleSessionTimeOut" type="boolean"/>
                    <property name="isMFAEnabled" type="boolean"/>
                </property>
            </property>
        </requestbody>
    </request>
    <request path="/api/v2/createSecurityPolicy">
        <requestbody type="object">
          <property name="securityPolicy" type="object">
              <property name="name" type="string" maxLength="255"/>
              <property name="description" type="string" maxLength="500"/>
              <property name="isDefault" type="boolean"/>
              <property name="pwdIsMixed" type="boolean"/>
              <property name="isWebSessManagementEnabled" type="boolean"/>
              <property name="isTOTPEnabled" type="boolean"/>
              <property name="isPwdPolicyEnabled" type="boolean"/>
              <property name="isMOTPEnabled" type="boolean"/>
              <property name="pwdMinAge" type="number"/>
              <property name="pwdMinLength" type="number"/>
              <property name="pwdMinNumDigits" type="number"/>
              <property name="pwdMinSplChars" type="number"/>
              <property name="pwdPrevPassRefusal" type="number"/>
              <property name="webSessLifeTime" type="number"/>
          </property>
        </requestbody>
    </request>
</config>
