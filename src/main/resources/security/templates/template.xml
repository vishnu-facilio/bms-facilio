    <template name="newPermissions">
        <items type="object">
            <property name="permission" type="number" mandatory="true"/>
            <property name="permission2" type="number"/>
            <property name="roleId" type="number" mandatory="true"/>
            <property name="tabId" type="number" mandatory="true"/>
        </items>
    </template>

    <template name="criteria">
            <property name="pattern" type="string"/>
            <property name="conditions" type="object" key-type="number" value-type="object" value-template="template.xml#conditions"/>     <!--conditions is Object of objects type [template]-->
            <property name="resourceOperator" type="boolean"/>
    </template>

    <template name="filters">
            <property name="subject" type="object">
                <property name="operatorId" type="number"/>
                <property name="value" type="array">
                    <items type="string"/>
                </property>
                <property name="operatorLookupModule" type="number"/>       <!--what is operatorLookupModule-->
            </property>
        <property name="oneLevelLookup" type="object">                      <!--How to define  oneLevelLookup in PostAuth-->
        </property>
    </template>

    <template name="role">
